print("Check out this dope calculator, yo")
mads = input("would you like to add, subtract, multiply, or divide?")

def div_func():
div = input("Enter a number")
div2 = input("Enter a second number")
div_answer = div / div2
print(div_answer)

if(mads == "Divide" | "divide" | "d" | "/"):div_func()

def multi_func():
mutli = input("Enter a number")
